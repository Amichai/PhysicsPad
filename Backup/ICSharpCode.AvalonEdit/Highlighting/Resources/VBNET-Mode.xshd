			<Word>New</Word>
			<Word>Not</Word>
			<Word>Or</Word>
			<Word>OrElse</Word>
			<Word>Xor</Word>
		</Keywords>
		<Keywords color="Constants">
			<Word>False</Word>
			<Word>Me</Word>
			<Word>MyBase</Word>
			<Word>MyClass</Word>
			<Word>Nothing</Word>
